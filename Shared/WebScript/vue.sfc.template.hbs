<template lang="pug">
{{{dom dom name 'sfc'}}}
</template>

<script>
{{#each refdComps}}
import {{{capitalized}}} from '{{{path.relative}}}'
{{/each}}

export default {
  name: '{{{capitalizedName}}}',
  components: {
    {{#each refdComps}}
    {{{capitalized}}},
    {{/each}}
  },
  mixins: [],
  props: {
    {{#each props}}
    {{@key}}: {
      default: {{{or this null}}},
    },
    {{/each}}
  },
  data() {
    return {
      {{#each data}}
      {{@key}}: {{{or this null}}},
      {{/each}}
    }
  },
  {{#if created}}
  created: {{{addCode created ""}}},
  {{/if}}
  {{#if mounted}}
  mounted: {{{addCode mounted ""}}},
  {{/if}}
  {{#if unmounted}}
  unmounted: {{{addCode unmounted ""}}},
  {{/if}}
  methods: {
    {{#each methods}}
    {{@key}}:
      {{{addCode this "" @key ""}}},
    {{/each}}
  },
  computed: {
    {{#each computed}}
    {{#if get}}
    {{@key}}: {
      get() {
        {{{addCode get ""}}}
      },
    },
    {{else}}
    {{@key}}: {{{addCode this "" @key ""}}},
    {{/if}}
    {{/each}}
  },
  watch: {
    {{#each watch}}
    {{#if handler}}
    {{@key}}: {
      handler(newValue, oldValue) {
        {{{addCode handler "" @key ""}}}
      },
      immediate: {{{or immediate false}}},
      deep: {{{or deep false}}},
    },
    {{else}}
    {{@key}}(newValue, oldValue) {
      {{{addCode this "" @key ""}}}
    },
    {{/if}}
    {{/each}}
  },
}
</script>

<style>
{{{css style}}}
</style>