<template lang="pug">
{{{dom dom name 'sfc'}}}
</template>

<script>
{{#each refdComps}}
import {{{capitalized}}} from '{{{path.relative}}}'
{{/each}}

export default {
  name: '{{{capitalizedName}}}',
  {{#if refdComps}}
  components: {
    {{#each refdComps}}
    {{{capitalized}}},
    {{/each}}
  },
  {{/if}}
  {{#if props}}
  props: {
    {{#each props}}
    {{@key}}: {
      default: {{{or this null}}},
    },
    {{/each}}
  },
  {{/if}}
  {{#if data}}
  data() {
    return {
      {{#each data}}
      {{@key}}: {{{or this null}}},
      {{/each}}
    }
  },
  {{/if}}
  {{#if created}}
  created: {{{addCode created ""}}},
  {{/if}}
  {{#if mounted}}
  mounted: {{{addCode mounted ""}}},
  {{/if}}
  {{#if beforeUnmount}}
  beforeUnmount: {{{addCode beforeUnmount ""}}},
  {{/if}}
  {{#if unmounted}}
  unmounted: {{{addCode unmounted ""}}},
  {{/if}}
  {{#if asyncData}}
  asyncData: {{{addCode asyncData ""}}},
  {{/if}}
  {{#if methods}}
  methods: {
    {{#each methods}}
    {{@key}}:
      {{{addCode this "" @key ""}}},
    {{/each}}
  },
  {{/if}}
  {{#if computed}}
  computed: {
    {{#each computed}}
    {{#if get}}
    {{@key}}: {
      get() {
        {{{addCode get ""}}}
      },
    },
    {{else}}
    {{@key}}: {{{addCode this "" @key ""}}},
    {{/if}}
    {{#if set}}
    {{@key}}: {{{addCode set "" @key ""}}},
    {{/if}}
    {{/each}}
  },
  {{/if}}
  {{#if watch}}
  watch: {
    {{#each watch}}
    {{#if handler}}
    {{@key}}: {
      handler: {{{addCode handler "" @key ""}}},
      immediate: {{{or immediate false}}},
      deep: {{{or deep false}}},
    },
    {{else}}
    {{@key}}: {{{addCode this "" @key ""}}},
    {{/if}}
    {{/each}}
  },
  {{/if}}
}
</script>

<style scoped>
{{{css style}}}
</style>