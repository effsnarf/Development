{
  mixins: [vueIdeCompMixin],
  props: {
    {{#each props}}
    {{@key}}: {
      default: {{{or this null}}}
    },
    {{/each}}
  },
  data: function () {
    return {
        {{#each data}}
        {{@key}}: {{{or this null}}},
        {{/each}}
        _: {
          comp: {
            name: `{{unkebabize name}}`,
          },
        }
    }
  },
  created: {{{addCode created ""}}},
  mounted: {{{addCode mounted ""}}},
  unmounted: {{{addCode unmounted ""}}},
  methods: {
    {{#each methods}}
    {{@key}}: {{{this}}},
    {{/each}}
  },
  computed: {
    {{#each computed}}
    {{#if get}}
    {{@key}}: {
      get: {{{get}}},
    },
    {{else}}
    {{@key}}: {{{this}}},
    {{/if}}
    {{/each}}
  },
  watch: {    
    {{#each watch}}
    {{#if handler}}
    {{@key}}: {
      handler: {{{handler}}},
      immediate: {{{or immediate false}}},
      deep: {{{or deep false}}},
    },
    {{else}}
    {{@key}}: {{{this}}},
    {{/if}}
    {{/each}}
  },
  template: `{{{dom dom name}}}`
}
