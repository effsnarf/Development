{
    "manifest_version": 3,
    "name": "4chan Sheakspearize",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["*://*.4chan.org/*"],
          "js": ["util.js", "sheakspearizer.js", "script.js"]
        }
    ],
    "background": {
      "service_worker": "service.worker.js"
    },
    "permissions": [
      "activeTab", "scripting"
    ]
  }
  