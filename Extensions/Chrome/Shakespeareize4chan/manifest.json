{
    "manifest_version": 3,
    "name": "Shakespeareize4chan",
    "version": "1.0",
    "description": "Shakespeareizes 4chan.",
    "content_scripts": [
        {
            "matches": ["https://*.4channel.org/*", "https://*.4chan.org/*"],
          "js": ["content.js"]
        }
      ],
    "background": {
        "service_worker": "background.js"
      }
  }
  